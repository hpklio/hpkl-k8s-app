examples {
  ["Rendering is working withot secret"] {
    new {
      name = "Test"
      id = 12121
      secret = null
    }
  }
  ["Rendering is working only secret"] {
    new {
      name = null
      id = null
      secret = "TestSecret"
    }
  }
  ["Rendering is working for inheritance"] {
    new {
      name = "Test"
      id = 12121
      secret = null
      another = "AnotherField"
      anotherSecret = null
    }
  }
  ["Rendering is working for inheritance only for secrets"] {
    new {
      name = null
      id = null
      secret = "TestSecret"
      another = null
      anotherSecret = "1212121"
    }
  }
  ["Listing secrets is working"] {
    new {
      list {
        new {
          name = null
          id = null
          secret = "TestSecret"
          another = null
          anotherSecret = "1212121"
        }
      }
    }
  }
  ["Mapping secrets is working"] {
    new {
      map {
        Test {
          name = null
          id = null
          secret = "TestSecret"
          another = null
          anotherSecret = "1212121"
        }
      }
    }
  }
  ["Lists of general types are working"] {
    new {
      list {
        "TEST"
        "TEST2"
      }
    }
  }
  ["Map of general types are working"] {
    new {
      map {
        TEST = "TEST"
        TEST2 = "TEST2"
      }
    }
  }
}
